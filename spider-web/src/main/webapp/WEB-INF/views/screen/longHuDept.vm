<div id="content">
  <div id="content-header">
    <div id="breadcrumb"> <a href="#" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> 首页</a> <a href="#">资讯管理</a> <a href="#" class="current">营业部历史龙虎榜</a> </div>
  </div>
  <div class="container-fluid">
    <div class="row-fluid">
      <div class="span12">
        <div class="widget-box">
          <div class="widget-title"> <span class="icon"><i class="icon-th"></i></span>
            <h5>$!{secDept.deptAddr}</h5>
			<a class="label label-info" style="line-height:20px;"></a>
          </div>
          <div class="widget-content">
            <table class="table table-bordered table-striped">
              <thead>
                <tr>
				  <th style="width:5%;">序号</th>
                  <th>代码</th>
				  <th>股票</th>
				  <th>日期</th>
                  <th>收盘价</th>
				  <th>涨跌幅</th>
				  <th>换手率</th>
				  <th>买入(万)</th>
				  <th>卖出(万)</th>
				  <th>净买入(万)</th>
				  <th>帮派</th>
                </tr>
              </thead>
              <tbody>
				#foreach($detail in ${detailList})
                <tr symbol="$!{detail.symbol}">
				  <td>$!{velocityCount}</td>
                  <td><a class="lh_detail" onclick="javascript:;">$!{detail.symbol}</a></td>
                  <td><a class="lh_detail" onclick="javascript:;">$!{detail.stockName}</a></td>
				  <td>$!{detail.tradeDate}</td>
				  <td>$!{detail.price}</td>
				  <td>$!{detail.chgPercent}</td>
				  <td>$!{detail.turnover}</td>
				  <td style="color:red">$!{detail.buyAmt}</td>
				  <td style="color:green">$!{detail.sellAmt}</td>
				  <td>$!{detail.netBuy}</td>
				  <td></td>
                </tr>
				#end
              </tbody>
             </table>
			 <div id="page_nav"></div>
			</div>
          </div>
        </div>
    </div>
  </div>
</div>
<script type="text/javascript">
	$(function(){
		$(".lh_detail").click(function(){
		});
	});
</script>
